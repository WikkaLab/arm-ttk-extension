{
	"manifestVersion": 1,
	"id": "ARMTTKExtension",
	"version": "1.1.$buildnumber$",
	"name": "Run ARM TTK Tests",
	"description": "Run Azure Resource Manager Template Test Kit tests as part of your build or release process",
	"publisher": "DavidOlivaParedes",
	"repository": {
		"type": "git",
		"url": "https://github.com/WikkaLab/arm-ttk-extension"
	},
	"content": {
		"details": {
			"path": "README.md"
		},
		"license": {
			"path": "LICENSE"
		}
	},
	"tags": [
		"Azure Pipelines",
		"Azure",
		"Azure Resource Manager",
		"Tests"
	],
	"targets": [
		{
			"id": "Microsoft.VisualStudio.Services"
		}
	],
	"icons": {
		"default": "icon.png"
	},
	"galleryFlags": [
		"Public"
	],

	"categories": [
		"Azure Pipelines"
	],
	"contributions": [
		{
			"id": "RunARMTTKTests",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "task"
			}
		}
	],
	"links": {
		"getstarted": {
			"uri": "https://github.com/WikkaLab/arm-ttk-extension/blob/master/README.md"
		},
		"support": {
			"uri": "https://github.com/WikkaLab/arm-ttk-extension/issues"
		}
	},
	"files": [
		{
			"path": "task"
		}
	]
}
